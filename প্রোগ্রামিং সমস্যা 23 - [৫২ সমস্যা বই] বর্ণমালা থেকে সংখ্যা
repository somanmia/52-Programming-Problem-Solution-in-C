#include<stdio.h>
#include<string.h>
int main(){
int T,i,j,len;
scanf("%d",&T);
for(j=0;j<T;j++){
    char ch[102];
    scanf("%s",&ch);
    
    len=strlen(ch);
    for(i=0;i<len;i++){
            if(ch[i]=='A'){
                printf("1");
            }
            else if(ch[i]=='B'){
              printf("2");
            }
            else if(ch[i]=='C'){
              printf("3");
            }else if(ch[i]=='D'){
              printf("4");
            }
            else if(ch[i]=='E'){
              printf("5");
            }else if(ch[i]=='F'){
              printf("6");
            }
            else if(ch[i]=='G'){
              printf("7");
            }
            else if(ch[i]=='H'){
              printf("8");
            }

            else if(ch[i]=='I'){
              printf("9");
            }else if(ch[i]=='J'){
              printf("10");
            }else if(ch[i]=='K'){
              printf("11");
            }else if(ch[i]=='L'){
              printf("12");
            }else if(ch[i]=='M'){
              printf("13");
            }else if(ch[i]=='N'){
              printf("14");
            }else if(ch[i]=='O'){
              printf("15");
            }
            else if(ch[i]=='P'){
              printf("16");
            }else if(ch[i]=='Q'){
              printf("17");
            }else if(ch[i]=='R'){
              printf("18");
            }else if(ch[i]=='S'){
              printf("19");
            }else if(ch[i]=='T'){
              printf("20");
            }else if(ch[i]=='U'){
              printf("21");
            }else if(ch[i]=='V'){
              printf("22");
            }
            else if(ch[i]=='W'){
              printf("23");
            }else if(ch[i]=='X'){
              printf("24");
            }else if(ch[i]=='Y'){
              printf("25");
            }else if(ch[i]=='Z'){
              printf("26");
            }
    }
    printf("\n");

}

return 0;
}
